<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Subject name</th>
      <th>Subject code</th>
      <th>Description</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.subject_name %></td>
        <td><%= subject.subject_code %></td>
        <td><%= subject.description %></td>
        <td><%= subject.course %></td>
      </tr>
    <% end %>
  </tbody>
</table>
